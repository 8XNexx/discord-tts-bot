{
  "name": "discord-tts-bot",
  "version": "4.2.0",
  "description": "A simple Discord TTS Bot that uses the Google Translate TTS API.",
  "main": "./src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "debug": "node ./src/app.js --debug",
    "dev": "nodemon --exec npm start",
    "lint:errors-only": "node_modules/.bin/eslint src --quiet",
    "lint": "node_modules/.bin/eslint src",
    "deploy": "node ./src/command-deployer.js",
    "start-sharded": "node ./src/sharded-client.js"
  },
  "engines": {
    "node": ">=16.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonstar-x/discord-tts-bot.git"
  },
  "author": "moonstar-x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moonstar-x/discord-tts-bot/issues"
  },
  "homepage": "https://github.com/moonstar-x/discord-tts-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^1.10.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.18.0",
    "@greencoast/discord.js-extended": "^3.0.1",
    "@greencoast/djs-extended-data-provider-level": "^1.0.1",
    "@greencoast/djs-extended-data-provider-redis": "^1.0.0",
    "@greencoast/logger": "^1.1.1",
    "axios": "^1.7.9",
    "deepmerge": "^4.3.1",
    "discord.js": "^14.17.2",
    "google-tts-api": "^2.0.2",
    "level": "^9.0.0",
    "redis": "^4.7.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-greencoast": "^0.0.5",
    "nodemon": "^3.1.9"
  }
}
